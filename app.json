{
  "expo": {
    "name": "CycleBees",
    "slug": "cyclebees",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "cyclebees",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD11E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyclebees.app",
      "infoPlist": {
        "NSCameraUsageDescription": "CycleBees needs camera access to allow you to take photos of your bicycle for repair services.",
        "NSPhotoLibraryUsageDescription": "CycleBees needs photo library access to let you select images of your bicycle for repair services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFD11E"
      },
      "package": "com.cyclebees.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
